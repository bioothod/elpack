/home/admin/elliptics/log/*.log /srv/elliptics/log/*.log /var/log/elliptics/*.log
{
	daily
	maxsize 2G
	missingok
	copytruncate
	compress
	dateext
	dateformat -%Y%m%d-%s
	rotate 300
}
